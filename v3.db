Table Etudiants {
  id_etudiants int [primary key]
  nb_immatriculation int
  nom varchar
  prenom varchar
  email varchar
  natel varchar
}

Table Professeurs {
  id_professeurs int [primary key]
  nom varchar 
  prenom varchar
  bureau varchar 
  email varchar
}

Table Cours {
  id_cours int [primary key]
  nom varchar 
  id_professeurs int 
}

Ref: Professeurs.id_professeurs < Cours.id_professeurs

Table Inscriptions {
  id_cours int 
  id_etudiants int 
}

Ref: Cours.id_cours > Inscriptions.id_cours
Ref: Etudiants.id_etudiants < Inscriptions.id_etudiants

Table Evaluations {
  id_evaluation int [primary key]
  id_etudiants int 
  id_cours int 
  date_evaluation date
  note decimal
  ponderation decimal
}

Ref: Inscriptions.id_etudiants < Evaluations.id_etudiants
Ref: Inscriptions.id_cours < Evaluations.id_cours

Table Assistants {
  id_assistants int [primary key]
  id_etudiants int 
  id_cours int 
  numero_bureau integer 
}

Ref: Etudiants.id_etudiants > Assistants.id_etudiants
Ref: Cours.id_cours < Assistants.id_cours
